<%= form_for(@folio) do |f| %>

<%= render "errors"  %>

  <div class="field hide">
    <%= f.label :user_id %><br>
    <%= f.number_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>

  </div>
  <div class="actions">
    <%= f.submit "Submit", class: :"waves-effect waves-light btn"  %>
  </div>
<% end %>
