<div class="container clearfix">

<p id="notice"><%= notice %></p>
<button id="view-toggle" type="button">Toggle View</button>
<div class="row">
  <div class="col s12 m8 elements" id="container">
    <% @elements.each do |element| %>
      <%= render "elements/element", element: element %>
    <% end %>
  </div>
  <div class="col s12 m4" id="sidebar">
    <%= render "elements/form"  %>
  </div>
</div>
<div id="tabs" class="row">
    <div class="col s12">
      <ul class="tabs">
        <li class="tab col s3"><a href="#test1">Test 1</a></li>
        <li class="tab col s3"><a class="active" href="#test2">Test 2</a></li>
        <li class="tab col s3"><a href="#test3">Disabled Tab</a></li>
        <li class="tab col s3"><a href="#test4">Test 4</a></li>
      </ul>
    </div>
    <div id="tab-content">
      <div id="test1" class="col s12">Test 1</div>
      <div id="test2" class="col s12">Test 2</div>
      <div id="test3" class="col s12">Test 3</div>
      <div id="test4" class="col s12">Test 4</div>
    </div>
  </div>
</div>
